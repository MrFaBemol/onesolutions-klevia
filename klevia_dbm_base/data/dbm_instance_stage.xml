<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="dbm_instance_stage_new" model="dbm.instance.stage">
            <field name="name">New</field>
            <field name="sequence">10</field>
            <field name="protected">True</field>
            <field name="decoration_type">muted</field>
        </record>

        <record id="dbm_instance_stage_setup" model="dbm.instance.stage">
            <field name="name">Setup</field>
            <field name="sequence">20</field>
            <field name="protected" eval="True" />
            <field name="decoration_type">info</field>
        </record>

        <record id="dbm_instance_stage_active" model="dbm.instance.stage">
            <field name="name">Active</field>
            <field name="sequence">30</field>
            <field name="protected" eval="True" />
            <field name="decoration_type">success</field>
        </record>

        <record id="dbm_instance_stage_archived" model="dbm.instance.stage">
            <field name="name">Inactive</field>
            <field name="sequence">90</field>
            <field name="fold" eval="True" />
            <field name="protected" eval="True" />
            <field name="decoration_type">danger</field>
        </record>

    </data>
</odoo>
